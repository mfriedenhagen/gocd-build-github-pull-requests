<?xml version="1.0" encoding="utf-8" ?>
<go-plugin id="gitlab.mr" version="1">
    <about>
        <name>GitLab Merge Requests Builder</name>
        <version>${project.version}</version>
        <target-go-version>${go.version}</target-go-version>
        <description>Plugin that polls a GitLab repository for merge requests and triggers a build for each of them</description>
        <vendor>
            <name>Mirko Friedenhagen</name>
            <url>https://github.com/mfriedenhagen/gocd-build-github-pull-requests</url>
        </vendor>
    </about>
</go-plugin>